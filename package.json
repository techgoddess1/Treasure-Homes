{
  "name": "treasure_homes",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && mkdir dist && babel server -d dist --copy-files",
    "clean": "rm -rf dist",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "dev": "nodemon",
    "start": "npm run build && node bin/prod",
    "test": "nyc --reporter=html --reporter=text mocha -r babel-register -r babel-polyfill -r dotenv/config server/test/*.js --exit"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.0",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^5.0.0",
    "cloudinary": "^1.14.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.0",
    "jsonwebtoken": "^8.5.1",
    "kickbox": "^2.0.4",
    "nodemailer": "^6.2.1",
    "pg": "^7.11.0",
    "swagger-ui-express": "^4.0.4"
  },
  "nodemonConfig": {
    "ignore": [
      "server/test/*.js"
    ],
    "watch": [
      "server"
    ],
    "exec": "node bin/dev",
    "ext": "js"
  },
  "engines": {
    "node": "10.16.3"
  }
}
